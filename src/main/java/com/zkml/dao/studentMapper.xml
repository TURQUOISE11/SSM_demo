<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zkml.dao.StudentDao">
    <insert id="save" parameterType="com.zkml.bean.Student">
    insert intot_app_student(id,name,sex,age) values(#{id},#{name},#{sex},#{age})
</insert>
<select id="getUser" resultType="java.lang.Integer" parameterType="java.lang.String">
    select COUNT(*) from tb_user
</select>

        </mapper>